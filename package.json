{
  "name": "mpa-liff-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build:purgecss": "node --no-warnings --experimental-loader=ts-node/esm --es-module-specifier-resolution=node src/build/purge-css.ts",
    "build:json": "NODE_ENV=build:json node --no-warnings --experimental-loader=ts-node/esm --es-module-specifier-resolution=node src/build/build-json.ts",
    "build:apps": "vue-tsc --noEmit && vite build",
    "build": "run-s build:{json,apps}",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,vue} --ignore-path .gitignore --max-warnings 0"
  },
  "dependencies": {
    "@line/liff": "^2.19.1",
    "@line/liff-mock": "^1.0.1",
    "daisyui": "^2.15.0",
    "dayjs": "^1.11.2",
    "glob": "^8.0.3",
    "got": "^12.0.4",
    "ky": "^0.30.0",
    "vue": "^3.2.35",
    "vue-query": "^1.24.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "eslint-plugin-vue": "^9.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.0",
    "typescript": "^4.6.4",
    "url": "^0.11.0",
    "vite": "^2.9.9",
    "vite-plugin-pages": "^0.23.0",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.15"
  }
}
